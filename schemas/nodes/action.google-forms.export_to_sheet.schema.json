{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://apps-script-studio.com/schemas/nodes/action.google-forms.export_to_sheet.schema.json",
  "title": "Google Forms - Export to Sheet",
  "description": "Export form responses to Google Sheets",
  "type": "object",
  "required": [
    "id",
    "type",
    "position",
    "parameters"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]+$",
      "description": "Unique identifier for the node"
    },
    "type": {
      "type": "string",
      "const": "action.google-forms.export_to_sheet",
      "description": "Node type identifier"
    },
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "additionalProperties": false
    },
    "parameters": {
      "type": "object",
      "properties": {
        "formId": {
          "description": "Form ID",
          "type": "string"
        },
        "spreadsheetId": {
          "description": "Target spreadsheet ID",
          "type": "string"
        }
      },
      "required": [
        "formId",
        "spreadsheetId"
      ],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "const": "Productivity"
        },
        "appName": {
          "type": "string",
          "const": "Google Forms"
        },
        "requiredScopes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "https://www.googleapis.com/auth/forms",
            "https://www.googleapis.com/auth/spreadsheets"
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}