{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://apps-script-studio.com/schemas/nodes/action.asana-enhanced.add_task_to_project.schema.json",
  "title": "Asana Enhanced - Add Task to Project",
  "description": "Add an existing task to a project",
  "type": "object",
  "required": [
    "id",
    "type",
    "position",
    "parameters"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]+$",
      "description": "Unique identifier for the node"
    },
    "type": {
      "type": "string",
      "const": "action.asana-enhanced.add_task_to_project",
      "description": "Node type identifier"
    },
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "additionalProperties": false
    },
    "parameters": {
      "type": "object",
      "properties": {
        "task_gid": {
          "type": "string",
          "description": "Task global ID"
        },
        "project": {
          "type": "string",
          "description": "Project ID to add the task to"
        },
        "insert_after": {
          "type": "string",
          "description": "Task ID to insert after"
        },
        "insert_before": {
          "type": "string",
          "description": "Task ID to insert before"
        }
      },
      "required": [
        "task_gid",
        "project"
      ],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "const": "Project Management"
        },
        "appName": {
          "type": "string",
          "const": "Asana Enhanced"
        },
        "requiredScopes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": []
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}