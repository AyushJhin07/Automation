{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://apps-script-studio.com/schemas/nodes/action.caldotcom.list_bookings.schema.json",
  "title": "Cal.com - List Bookings",
  "description": "List user bookings",
  "type": "object",
  "required": [
    "id",
    "type",
    "position",
    "parameters"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]+$",
      "description": "Unique identifier for the node"
    },
    "type": {
      "type": "string",
      "const": "action.caldotcom.list_bookings",
      "description": "Node type identifier"
    },
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "additionalProperties": false
    },
    "parameters": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "number",
          "description": "Filter by user ID"
        },
        "eventTypeId": {
          "type": "number",
          "description": "Filter by event type ID"
        },
        "status": {
          "type": "string",
          "enum": [
            "accepted",
            "pending",
            "cancelled",
            "rejected"
          ],
          "description": "Filter by booking status"
        },
        "take": {
          "type": "number",
          "minimum": 1,
          "maximum": 100,
          "default": 10,
          "description": "Number of bookings to return"
        },
        "skip": {
          "type": "number",
          "description": "Number of bookings to skip"
        },
        "orderBy": {
          "type": "string",
          "enum": [
            "createdAt",
            "startTime",
            "endTime"
          ],
          "description": "Order by field"
        },
        "sortOrder": {
          "type": "string",
          "enum": [
            "asc",
            "desc"
          ],
          "description": "Sort order"
        }
      },
      "required": [],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "const": "Scheduling"
        },
        "appName": {
          "type": "string",
          "const": "Cal.com"
        },
        "requiredScopes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": []
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}