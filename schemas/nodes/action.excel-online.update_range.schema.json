{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://apps-script-studio.com/schemas/nodes/action.excel-online.update_range.schema.json",
  "title": "Microsoft Excel Online - Update Range",
  "description": "Update values in a cell range",
  "type": "object",
  "required": [
    "id",
    "type",
    "position",
    "parameters"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]+$",
      "description": "Unique identifier for the node"
    },
    "type": {
      "type": "string",
      "const": "action.excel-online.update_range",
      "description": "Node type identifier"
    },
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "additionalProperties": false
    },
    "parameters": {
      "type": "object",
      "properties": {
        "driveId": {
          "type": "string",
          "description": "Drive ID"
        },
        "itemId": {
          "type": "string",
          "description": "Workbook item ID"
        },
        "worksheetId": {
          "type": "string",
          "description": "Worksheet ID or name"
        },
        "address": {
          "type": "string",
          "description": "Cell range address (e.g., A1:C10)"
        },
        "values": {
          "type": "array",
          "items": {
            "type": "array"
          },
          "description": "2D array of values to set"
        },
        "numberFormat": {
          "type": "array",
          "items": {
            "type": "array"
          },
          "description": "2D array of number formats"
        }
      },
      "required": [
        "driveId",
        "itemId",
        "worksheetId",
        "address",
        "values"
      ],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "const": "Productivity"
        },
        "appName": {
          "type": "string",
          "const": "Microsoft Excel Online"
        },
        "requiredScopes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": []
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}