{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://apps-script-studio.com/schemas/nodes/action.excel-online.get_range.schema.json",
  "title": "Excel Online - Get Range",
  "description": "Get values from a range in Excel",
  "type": "object",
  "required": [
    "id",
    "type",
    "position",
    "parameters"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]+$",
      "description": "Unique identifier for the node"
    },
    "type": {
      "type": "string",
      "const": "action.excel-online.get_range",
      "description": "Node type identifier"
    },
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "additionalProperties": false
    },
    "parameters": {
      "type": "object",
      "properties": {
        "workbookId": {
          "description": "Workbook ID",
          "type": "string"
        },
        "worksheet": {
          "description": "Worksheet name",
          "type": "string"
        },
        "rangeA1": {
          "description": "A1 notation range",
          "type": "string"
        }
      },
      "required": [
        "workbookId",
        "worksheet",
        "rangeA1"
      ],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "const": "Productivity"
        },
        "appName": {
          "type": "string",
          "const": "Excel Online"
        },
        "requiredScopes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "Files.Read"
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}