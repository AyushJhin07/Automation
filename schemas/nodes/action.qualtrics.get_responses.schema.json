{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://apps-script-studio.com/schemas/nodes/action.qualtrics.get_responses.schema.json",
  "title": "Qualtrics - Get Responses",
  "description": "Get survey responses",
  "type": "object",
  "required": [
    "id",
    "type",
    "position",
    "parameters"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]+$",
      "description": "Unique identifier for the node"
    },
    "type": {
      "type": "string",
      "const": "action.qualtrics.get_responses",
      "description": "Node type identifier"
    },
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "additionalProperties": false
    },
    "parameters": {
      "type": "object",
      "properties": {
        "surveyId": {
          "type": "string",
          "description": "Survey ID"
        },
        "skipToken": {
          "type": "string",
          "description": "Pagination token"
        },
        "useLabels": {
          "type": "boolean",
          "default": false,
          "description": "Use labels instead of values"
        },
        "limit": {
          "type": "number",
          "minimum": 1,
          "maximum": 2500,
          "default": 100,
          "description": "Number of responses to return"
        },
        "includedQuestionIds": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "Include specific question IDs"
        },
        "startDate": {
          "type": "string",
          "format": "date-time",
          "description": "Start date filter"
        },
        "endDate": {
          "type": "string",
          "format": "date-time",
          "description": "End date filter"
        }
      },
      "required": [
        "surveyId"
      ],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "const": "Surveys"
        },
        "appName": {
          "type": "string",
          "const": "Qualtrics"
        },
        "requiredScopes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": []
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}