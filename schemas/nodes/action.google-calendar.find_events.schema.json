{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://apps-script-studio.com/schemas/nodes/action.google-calendar.find_events.schema.json",
  "title": "Google Calendar - Find Events",
  "description": "Search for events",
  "type": "object",
  "required": [
    "id",
    "type",
    "position",
    "parameters"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]+$",
      "description": "Unique identifier for the node"
    },
    "type": {
      "type": "string",
      "const": "action.google-calendar.find_events",
      "description": "Node type identifier"
    },
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "additionalProperties": false
    },
    "parameters": {
      "type": "object",
      "properties": {
        "calendarId": {
          "description": "Calendar ID",
          "type": "string"
        },
        "query": {
          "description": "Search query",
          "type": "string"
        },
        "timeMin": {
          "description": "Start time filter",
          "type": "string"
        },
        "timeMax": {
          "description": "End time filter",
          "type": "string"
        }
      },
      "required": [],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "const": "Productivity"
        },
        "appName": {
          "type": "string",
          "const": "Google Calendar"
        },
        "requiredScopes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "https://www.googleapis.com/auth/calendar.readonly"
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}