{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://apps-script-studio.com/schemas/nodes/action.mailchimp.get_lists.schema.json",
  "title": "Mailchimp - Get Lists",
  "description": "Get all audience lists",
  "type": "object",
  "required": [
    "id",
    "type",
    "position",
    "parameters"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_-]+$",
      "description": "Unique identifier for the node"
    },
    "type": {
      "type": "string",
      "const": "action.mailchimp.get_lists",
      "description": "Node type identifier"
    },
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "additionalProperties": false
    },
    "parameters": {
      "type": "object",
      "properties": {
        "count": {
          "type": "number",
          "minimum": 1,
          "maximum": 1000,
          "default": 10,
          "description": "Number of lists to return"
        },
        "offset": {
          "type": "number",
          "minimum": 0,
          "default": 0,
          "description": "Number of lists to skip"
        },
        "beforeDateCreated": {
          "type": "string",
          "format": "date-time",
          "description": "Filter lists created before this date"
        },
        "sinceDateCreated": {
          "type": "string",
          "format": "date-time",
          "description": "Filter lists created after this date"
        }
      },
      "required": [],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "const": "Marketing"
        },
        "appName": {
          "type": "string",
          "const": "Mailchimp"
        },
        "requiredScopes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": []
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}