    throw new Error('Credential token metadata mismatch for ' + (token.purpose || 'credential') + '.');
  }

  return {
    payload: sealedPayload.payload,
    issuedAt: token.issuedAt,
    expiresAt: token.expiresAt,
    purpose: token.purpose || null,
