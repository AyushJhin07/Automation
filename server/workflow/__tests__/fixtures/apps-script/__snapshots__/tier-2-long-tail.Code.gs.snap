  if (
    !sealedPayload ||
    typeof sealedPayload !== 'object' ||
    sealedPayload.issuedAt !== token.issuedAt ||
    sealedPayload.expiresAt !== token.expiresAt ||
    (sealedPayload.purpose || null) !== (token.purpose || null)
