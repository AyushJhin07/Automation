      } catch (innerError) {
        evaluationError = innerError && innerError.message ? innerError.message : String(innerError);
      }
    } else {
      rawValue = false;
    }
  } catch (error) {
    evaluationError = error && error.message ? error.message : String(error);
  }

  if (typeof rawValue === 'undefined') {
