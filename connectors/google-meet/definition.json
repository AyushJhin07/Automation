{
  "id": "google-meet",
  "name": "Google Meet",
  "description": "Google Meet for video conferencing and meetings",
  "category": "Communication",
  "icon": "google-meet",
  "color": "#00ACC1",
  "version": "1.0.0",
  "authentication": {
    "type": "oauth2",
    "config": {
      "authUrl": "https://accounts.google.com/o/oauth2/v2/auth",
      "tokenUrl": "https://oauth2.googleapis.com/token",
      "scopes": [
        "https://www.googleapis.com/auth/meetings.space.created",
        "https://www.googleapis.com/auth/meetings.space.readonly"
      ]
    }
  },
  "baseUrl": "https://meet.googleapis.com/v2",
  "actions": [
    {
      "id": "test_connection",
      "name": "Test Connection",
      "description": "Test the connection to Google Meet",
      "parameters": {
        "type": "object",
        "properties": {},
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "create_space",
      "name": "Create Space",
      "description": "Create a new meeting space",
      "parameters": {
        "type": "object",
        "properties": {
          "config": {
            "type": "object",
            "properties": {
              "accessType": {
                "type": "string",
                "enum": [
                  "OPEN",
                  "TRUSTED",
                  "RESTRICTED"
                ],
                "default": "TRUSTED",
                "description": "Access type for the space"
              },
              "entryPointAccess": {
                "type": "string",
                "enum": [
                  "ALL",
                  "CREATOR_APP_ONLY"
                ],
                "default": "ALL",
                "description": "Entry point access"
              }
            },
            "description": "Space configuration"
          }
        },
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "get_space",
      "name": "Get Space",
      "description": "Get meeting space details",
      "parameters": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Space name (spaces/SPACE_ID)"
          }
        },
        "required": [
          "name"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "end_active_conference",
      "name": "End Active Conference",
      "description": "End the active conference in a space",
      "parameters": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Space name (spaces/SPACE_ID)"
          }
        },
        "required": [
          "name"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "list_conference_records",
      "name": "List Conference Records",
      "description": "List conference records for a space",
      "parameters": {
        "type": "object",
        "properties": {
          "parent": {
            "type": "string",
            "description": "Parent space (spaces/SPACE_ID)"
          },
          "pageSize": {
            "type": "number",
            "minimum": 1,
            "maximum": 100,
            "default": 25,
            "description": "Number of records to return"
          },
          "pageToken": {
            "type": "string",
            "description": "Page token for pagination"
          },
          "filter": {
            "type": "string",
            "description": "Filter expression"
          }
        },
        "required": [
          "parent"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "get_conference_record",
      "name": "Get Conference Record",
      "description": "Get a specific conference record",
      "parameters": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Conference record name"
          }
        },
        "required": [
          "name"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "list_participants",
      "name": "List Participants",
      "description": "List participants in a conference",
      "parameters": {
        "type": "object",
        "properties": {
          "parent": {
            "type": "string",
            "description": "Conference record name"
          },
          "pageSize": {
            "type": "number",
            "minimum": 1,
            "maximum": 100,
            "default": 25,
            "description": "Number of participants to return"
          },
          "pageToken": {
            "type": "string",
            "description": "Page token for pagination"
          },
          "filter": {
            "type": "string",
            "description": "Filter expression"
          }
        },
        "required": [
          "parent"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "get_participant",
      "name": "Get Participant",
      "description": "Get details about a specific participant",
      "parameters": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Participant name"
          }
        },
        "required": [
          "name"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "list_participant_sessions",
      "name": "List Participant Sessions",
      "description": "List sessions for a participant",
      "parameters": {
        "type": "object",
        "properties": {
          "parent": {
            "type": "string",
            "description": "Participant name"
          },
          "pageSize": {
            "type": "number",
            "minimum": 1,
            "maximum": 100,
            "default": 25,
            "description": "Number of sessions to return"
          },
          "pageToken": {
            "type": "string",
            "description": "Page token for pagination"
          },
          "filter": {
            "type": "string",
            "description": "Filter expression"
          }
        },
        "required": [
          "parent"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "get_participant_session",
      "name": "Get Participant Session",
      "description": "Get details about a participant session",
      "parameters": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Participant session name"
          }
        },
        "required": [
          "name"
        ],
        "additionalProperties": false
      }
    }
  ],
  "triggers": [
    {
      "id": "space_created",
      "name": "Space Created",
      "description": "Triggered when a meeting space is created",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {},
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "meetingUri": {
            "type": "string"
          },
          "meetingCode": {
            "type": "string"
          },
          "config": {
            "type": "object"
          },
          "activeConference": {
            "type": "object"
          }
        }
      }
    },
    {
      "id": "conference_started",
      "name": "Conference Started",
      "description": "Triggered when a conference starts",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {
          "space": {
            "type": "string",
            "description": "Filter by specific space"
          }
        },
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "startTime": {
            "type": "string"
          },
          "endTime": {
            "type": "string"
          },
          "space": {
            "type": "string"
          }
        }
      }
    },
    {
      "id": "participant_joined",
      "name": "Participant Joined",
      "description": "Triggered when a participant joins",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {
          "space": {
            "type": "string",
            "description": "Filter by specific space"
          }
        },
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "signedinUser": {
            "type": "object"
          },
          "anonymousUser": {
            "type": "object"
          },
          "phoneUser": {
            "type": "object"
          },
          "earliestStartTime": {
            "type": "string"
          },
          "latestEndTime": {
            "type": "string"
          }
        }
      }
    }
  ],
  "testConnection": {
    "method": "GET",
    "endpoint": "/spaces"
  },
  "release": {
    "semver": "1.0.0",
    "status": "stable",
    "isBeta": false,
    "betaStartedAt": null,
    "deprecationWindow": {
      "startDate": null,
      "sunsetDate": null
    }
  }
}
