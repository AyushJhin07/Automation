{
  "id": "typeform",
  "name": "Typeform",
  "description": "Online form builder for creating surveys, quizzes, and questionnaires",
  "category": "Customer Feedback",
  "icon": "typeform",
  "color": "#262627",
  "version": "1.0.0",
  "authentication": {
    "type": "api_key",
    "config": {
      "apiKeyLocation": "header",
      "apiKeyName": "Authorization",
      "apiKeyValue": "Bearer {api_key}"
    }
  },
  "baseUrl": "https://api.typeform.com",
  "actions": [],
  "triggers": [
    {
      "id": "form_response",
      "name": "Form Response",
      "description": "Triggered when a form response is submitted",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {
          "uid": {
            "type": "string",
            "description": "Form UID to monitor"
          },
          "completed": {
            "type": "boolean",
            "description": "Filter by completion status"
          }
        },
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "event_id": {
            "type": "string"
          },
          "event_type": {
            "type": "string"
          },
          "form_response": {
            "type": "object"
          }
        }
      }
    },
    {
      "id": "form_created",
      "name": "Form Created",
      "description": "Triggered when a new form is created",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {},
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "last_updated_at": {
            "type": "string"
          }
        }
      }
    }
  ],
  "actions_append": [
    {
      "id": "delete_webhook",
      "name": "Delete Webhook",
      "description": "Delete a webhook for a form",
      "endpoint": "/forms/{uid}/webhooks/{tag}",
      "method": "DELETE",
      "parameters": {
        "type": "object",
        "properties": {
          "uid": {
            "type": "string"
          },
          "tag": {
            "type": "string",
            "default": "default"
          }
        },
        "required": [
          "uid"
        ],
        "additionalProperties": false
      }
    }
  ],
  "testConnection": {
    "method": "GET",
    "endpoint": "/me"
  },
  "semanticVersion": "1.0.0",
  "lifecycle": {
    "status": "ga",
    "beta": {
      "enabled": false,
      "startDate": null,
      "endDate": null
    },
    "deprecation": {
      "startDate": null,
      "endDate": null
    },
    "sunsetDate": null
  }
}
