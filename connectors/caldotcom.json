{
  "id": "caldotcom",
  "name": "Cal.com",
  "description": "Cal.com open-source scheduling platform",
  "category": "Scheduling",
  "icon": "cal-com",
  "color": "#111827",
  "version": "1.0.0",
  "authentication": {
    "type": "api_key",
    "config": {
      "type": "header",
      "name": "Authorization",
      "prefix": "Bearer"
    }
  },
  "baseUrl": "https://api.cal.com/v1",
  "actions": [
    {
      "id": "test_connection",
      "name": "Test Connection",
      "description": "Test the connection to Cal.com",
      "parameters": {
        "type": "object",
        "properties": {},
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "create_booking",
      "name": "Create Booking",
      "description": "Create a new booking",
      "parameters": {
        "type": "object",
        "properties": {
          "eventTypeId": {
            "type": "number",
            "description": "Event type ID"
          },
          "start": {
            "type": "string",
            "format": "date-time",
            "description": "Booking start time"
          },
          "end": {
            "type": "string",
            "format": "date-time",
            "description": "Booking end time"
          },
          "attendee": {
            "type": "object",
            "properties": {
              "name": {"type": "string"},
              "email": {"type": "string", "format": "email"},
              "timeZone": {"type": "string"}
            },
            "required": ["name", "email"],
            "description": "Attendee information"
          },
          "meetingUrl": {
            "type": "string",
            "format": "uri",
            "description": "Meeting URL"
          },
          "location": {
            "type": "string",
            "description": "Meeting location"
          },
          "notes": {
            "type": "string",
            "description": "Additional notes"
          },
          "customInputs": {
            "type": "object",
            "description": "Custom form inputs"
          }
        },
        "required": ["eventTypeId", "start", "end", "attendee"],
        "additionalProperties": false
      }
    },
    {
      "id": "get_bookings",
      "name": "Get Bookings",
      "description": "Retrieve bookings",
      "parameters": {
        "type": "object",
        "properties": {
          "take": {
            "type": "number",
            "minimum": 1,
            "maximum": 100,
            "default": 10,
            "description": "Number of bookings to return"
          },
          "skip": {
            "type": "number",
            "description": "Number of bookings to skip"
          },
          "status": {
            "type": "string",
            "enum": ["upcoming", "recurring", "past", "cancelled"],
            "description": "Filter by status"
          },
          "attendeeEmail": {
            "type": "string",
            "format": "email",
            "description": "Filter by attendee email"
          },
          "eventTypeId": {
            "type": "number",
            "description": "Filter by event type"
          },
          "uid": {
            "type": "string",
            "description": "Specific booking UID"
          }
        },
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "cancel_booking",
      "name": "Cancel Booking",
      "description": "Cancel a booking",
      "parameters": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "Booking ID"
          },
          "uid": {
            "type": "string",
            "description": "Booking UID"
          },
          "reason": {
            "type": "string",
            "description": "Cancellation reason"
          }
        },
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "get_event_types",
      "name": "Get Event Types",
      "description": "Retrieve event types",
      "parameters": {
        "type": "object",
        "properties": {
          "take": {
            "type": "number",
            "minimum": 1,
            "maximum": 100,
            "default": 10,
            "description": "Number of event types to return"
          },
          "skip": {
            "type": "number",
            "description": "Number of event types to skip"
          },
          "username": {
            "type": "string",
            "description": "Filter by username"
          }
        },
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "get_availability",
      "name": "Get Availability",
      "description": "Get availability for an event type",
      "parameters": {
        "type": "object",
        "properties": {
          "eventTypeId": {
            "type": "number",
            "description": "Event type ID"
          },
          "dateFrom": {
            "type": "string",
            "format": "date",
            "description": "Start date for availability"
          },
          "dateTo": {
            "type": "string",
            "format": "date",
            "description": "End date for availability"
          },
          "timeZone": {
            "type": "string",
            "description": "Time zone"
          }
        },
        "required": ["eventTypeId"],
        "additionalProperties": false
      }
    }
  ],
  "triggers": [
    {
      "id": "booking_created",
      "name": "Booking Created",
      "description": "Triggered when a new booking is created",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {
          "eventTypeId": {
            "type": "number",
            "description": "Filter by event type"
          }
        },
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "id": {"type": "number"},
          "uid": {"type": "string"},
          "title": {"type": "string"},
          "startTime": {"type": "string"},
          "endTime": {"type": "string"},
          "attendees": {"type": "array"}
        }
      }
    },
    {
      "id": "booking_cancelled",
      "name": "Booking Cancelled",
      "description": "Triggered when a booking is cancelled",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {},
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "id": {"type": "number"},
          "uid": {"type": "string"},
          "reason": {"type": "string"}
        }
      }
    }
  ],
  "testConnection": {
    "method": "GET",
    "endpoint": "/me"
  }
}