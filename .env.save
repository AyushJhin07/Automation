# Apps Script Studio - Environment Variables
# Copy this file to .env and fill in your actual API keys

# LLM Provider API Keys (for AI features)
OPENAI_API_KEY=test-key-for-development
ANTHROPIC_API_KEY=
CLAUDE_API_KEY=
GOOGLE_API_KEY=

# Database Configuration (commented out for testing)
# DATABASE_URL="sqlite://./dev.db"

# JWT Secret for authentication
JWT_SECRET="your-super-secret-jwt-key-here-change-in-production"

# Server Configuration
NODE_ENV=developmen
PORT="5000"

# Optional: Rate limiting and monitoring
RATE_LIMIT_ENABLED="true"
RATE_LIMIT_WINDOW_MS="900000"
RATE_LIMIT_MAX_REQUESTS="100"

# Optional: Feature flags
ENABLE_LLM_FEATURES="true"
ENABLE_VECTOR_DB="true"
ENABLE_FINE_TUNING="true"
